{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.0.json",
  "description": "Ozone overview",
  "data": {"url": "data/airquality2.csv"},
  "width": 420,
  "height": 240,
  "vconcat": [
  {
    "width": 420,
    "height": 120,
    "mark": "line",
  "encoding": {
    "x": {
      "timeUnit": "month", "field": "date", "type": "temporal",
      "axis": {"title": "Month"}
    },
    "y": {
      "aggregate": "mean", "field": "Ozone", "type": "quantitative",
      "axis": {"title": "Mean Ozone (PPB)"}
    }
  }
}, {
  "width": 420,
  "height": 120,
  "mark": "line",
"encoding": {
  "x": {
    "timeUnit": "month", "field": "date", "type": "temporal",
    "axis": {"title": "Month"}
  },
  "y": {
    "aggregate": "mean", "field": "Solar", "type": "quantitative",
    "axis": {"title": "Mean Solar (Langleys)"}
  }
}

}
]
}
